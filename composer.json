{
    "name": "sunchaser/wp-libravatar-replace",
    "description": "Libravatar Replace. A WordPress plugin that adds Libravatar avatars support to your WordPress installation",
    "keywords": ["libravatar", "avatar", "wordpress", "buddypress"],
    "type": "wordpress-plugin",
    "license": ["ISC"],
    "authors": [{
        "name": "Christian Archer",
        "email": "chrstnarchr@aol.com"
    }],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "pake": "pake",
        "readme": "wp2md -i readme.txt -o README.md"
    },
    "require": {
        "php": ">= 5.2",
        "pear/services_libravatar": ">= 0.2.3"
    },
    "require-dev": {
        "johnpbloch/wordpress": "*",
        "wpreadme2markdown/wp2md": ">= 3.0",
        "indeyets/pake": "~1.99",
        "sllh/composer-versions-check": "^2.0"
    },
    "conflict": {
        "wpackagist-plugin/libravatar": "*",
        "wpackagist-plugin/libravatar-replace": "*"
    }
}
